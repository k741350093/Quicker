<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.quicker"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_launcher"
        android:label="@string/app_name"
        android:theme="@style/notitle" >
        <activity
            android:name=".activity.WelcomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.LoginActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".activity.RegisterInfoActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".activity.RegisterPhoneActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".activity.RegisterResultActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".activity.MainReferanceActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".activity.NoticeActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".activity.InformActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".activity.ActivityActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".activity.ClassSituationActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".activity.FormActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".activity.WaitActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>